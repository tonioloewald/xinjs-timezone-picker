{
  "name": "xinjs-timezone-picker",
  "version": "0.2.0",
  "description": "Incredibly simple, powerful, and efficient state management for Reactâ€¦",
  "main": "dist/index.js",
  "types": "dist/types.d.ts",
  "scripts": {
    "start": "bun parcel demo/index.html --dist-dir www -p 8777",
    "build": "bun test && rm -rf docs && bun parcel build --public-url .",
    "test": "bun test",
    "update": "bun install"
  },
  "keywords": [
    "xinjs",
    "web-components",
    "javascript",
    "typescript",
    "timezones",
    "timezone-picker"
  ],
  "author": "Tonio Loewald",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tonioloewald/xinjs-timezone-picker.git"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.9.3",
    "@parcel/transformer-typescript-types": "^2.9.3",
    "bun-types": "latest",
    "parcel": "latest",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "xinjs": "^0.4.14"
  },
  "files": [
    "/cdn",
    "/dist",
    "/LICENSE",
    "/README.md"
  ],
  "type": "module",
  "targets": {
    "docs": {
      "context": "browser",
      "distDir": "docs",
      "source": "demo/index.html",
      "optimize": true
    },
    "dist": {
      "context": "browser",
      "source": "src/index.ts",
      "isLibrary": true,
      "distDir": "dist",
      "outputFormat": "esmodule"
    },
    "cdn": {
      "context": "browser",
      "source": "src/index.ts",
      "distDir": "cdn",
      "optimize": true,
      "isLibrary": true,
      "outputFormat": "esmodule"
    }
  }
}
